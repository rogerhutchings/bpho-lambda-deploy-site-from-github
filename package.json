{
  "name": "bpho-lambda-deploy-site-from-github",
  "version": "0.0.1",
  "description": "",
  "dependencies": {
    "async": "^2.1.4",
    "extract-zip": "^1.6.0",
    "request": "^2.79.0",
    "s3": "^4.4.0"
  },
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "copy-webpack-plugin": "^4.0.1",
    "webpack": "^1.14.0",
    "webpack-node-externals": "^1.5.4"
  },
  "scripts": {
    "_build": "npm run _clean && mkdir dist && npm run _webpack && npm run _commit",
    "_clean": "rm -rf ./dist && rm -f ./bpho-lambda-deploy-site-from-github.zip",
    "_commit": "git rev-parse --verify HEAD >> dist/deployed_commit.txt",
    "_update": "aws lambda update-function-code --function-name BPhODeploySiteFromGitHub --s3-bucket bpho-lambdas --s3-key bpho-lambda-deploy-site-from-github.zip --profile bpho",
    "_upload": "aws s3 cp ./bpho-lambda-deploy-site-from-github.zip s3://bpho-lambdas/bpho-lambda-deploy-site-from-github.zip --profile bpho",
    "_webpack": "webpack --config webpack.config.js",
    "_zip": "cd dist && zip -r -X ../bpho-lambda-deploy-site-from-github.zip .",
    "deploy": "npm run _build && npm run _zip && npm run _upload && npm run _update && npm run _clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rogerhutchings/bpho-lambda-deploy-site-from-github.git"
  },
  "author": "Roger Hutchings <roger@hutchin.gs> (https://www.rogerhutchin.gs/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/rogerhutchings/bpho-lambda-deploy-site-from-github/issues"
  },
  "homepage": "https://github.com/rogerhutchings/bpho-lambda-deploy-site-from-github#readme"
}
